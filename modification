var orchestrator = new MigrationOrchestrator(
    pureRelations: _pureRelations,
    extendedRelations: _extendedRelations,
    idMappings: _idMappings,
    getForeignKeyInfo: GetForeignKeyInfo,
    importTable: ImportTable,
    importPureRelationTable: ImportPureRelationTable,
    importExtendedRelationTable: ImportExtendedRelationTable,
    getAllTables: GetAllTables // à implémenter si nécessaire
);

orchestrator.PerformGlobalImport();