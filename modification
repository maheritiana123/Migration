-- Test A : est-ce que le split renvoie des lignes ?
SELECT value AS Ligne
FROM STRING_SPLIT(@ValuesBatch, NCHAR(30));

-- Test B : split des colonnes sur la premi√®re ligne pour valider 0x1D
DECLARE @firstLine nvarchar(max) =
(
  SELECT TOP 1 value
  FROM STRING_SPLIT(@ValuesBatch, NCHAR(30))
);

SELECT value AS Colonne
FROM STRING_SPLIT(@firstLine, NCHAR(29));