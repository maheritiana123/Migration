// Exemple de règles locales : table -> (col -> valeur de substitution)
private static readonly Dictionary<string, Dictionary<string, object>> _notNullFallbacks =
    new(StringComparer.OrdinalIgnoreCase)
{
    ["VarEnv"] = new(StringComparer.OrdinalIgnoreCase)
    {
        ["VENom"] = ""  // remplace NULL par chaîne vide
    },
    ["SMSSITECODE"] = new(StringComparer.OrdinalIgnoreCase)
    {
        ["Sms_Site_Name"] = "UNKNOWN"
    }
};

// ...
// après avoir rempli 'row' et résolu les FK :
if (_notNullFallbacks.TryGetValue(tableName, out var perCol))
{
    foreach (var kvp in perCol)
    {
        var col = kvp.Key;
        if (row.ContainsKey(col) && (row[col] == null || string.IsNullOrWhiteSpace(row[col]?.ToString())))
            row[col] = kvp.Value;
    }
}