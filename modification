DECLARE @ValuesBatch NVARCHAR(MAX) = N'''Venom_1''' + NCHAR(31) + N'123' + 
                                     NCHAR(30) + 
                                     N'''Venom_2''' + NCHAR(31) + N'456';

;WITH Numbers AS (
    SELECT TOP (LEN(@ValuesBatch))
           ROW_NUMBER() OVER (ORDER BY (SELECT NULL)) AS n
    FROM sys.objects
)
SELECT 
    n AS Position,
    SUBSTRING(@ValuesBatch, n, 1) AS Char,
    CONVERT(VARBINARY(1), SUBSTRING(@ValuesBatch, n, 1)) AS HexValue
FROM Numbers;