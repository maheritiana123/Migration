DECLARE @VB nvarchar(max) = /* colle ICI exactement ce que tu envoies */ N'...';

SELECT
  RS_Count = LEN(@VB) - LEN(REPLACE(@VB, NCHAR(30), N'')),
  CS_Count = LEN(@VB) - LEN(REPLACE(@VB, NCHAR(29), N''));

-- Aperçu lisible
SELECT REPLACE(REPLACE(@VB, NCHAR(30), N'␞'), NCHAR(29), N'␝') AS Preview;